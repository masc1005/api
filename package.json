{
  "name": "caveo",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typeorm": "typeorm-ts-node-esm",
    "migration:generate": "npm run typeorm -- migration:generate -d dist/config/database/database.config",
    "migration:run": "npm run typeorm -- migration:run -d dist/config/database/database.config",
    "migration:revert": "npm run typeorm -- migration:revert -d dist/config/database/database.config"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.645.0",
    "@koa/cors": "^5.0.0",
    "@koa/router": "^13.0.0",
    "aws-sdk": "^2.1689.0",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "koa": "^2.15.3",
    "koa-bodyparser": "^4.4.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-router": "^7.4.8",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__router": "^12.0.4",
    "@types/node": "^22.5.2",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0"
  }
}
